@model IEnumerable<FoodDefence.Models.TRAMPA>
<div >
    <table class="table table-bordered table-sm">
        <tr>
            <th class="priority-1">
                Tipo
            </th>
            <th class="priority-1">
                Número
            </th>
            <th class="priority-2">
                Estado
            </th>
            <th class="priority-3">
                Observaciones
            </th>
            <th class="priority-1"></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td class="priority-1">
                    @Html.DisplayFor(modelItem => item.TRAMPA_TIPO.descripcion)
                </td>
                <td class="priority-1">
                    @Html.DisplayFor(modelItem => item.numero)
                </td>
                <td class="priority-2">
                    @Html.DisplayFor(modelItem => item.TRAMPA_ESTADO.descripcion)
                </td>
                <td class="priority-3">
                    @Html.DisplayFor(modelItem => item.observaciones)
                </td>
                <td class="priority-1">
                    @Html.ActionLink("Editar", "Edit", "TRAMPA", new { id = item.id }, new { @class = "btn btn-outline-dark" })
                    @Html.ActionLink("Eliminar", "Delete", "TRAMPA", new { id = item.id }, new { @class = "btn btn-outline-danger" })
                </td>
            </tr>
        }
    </table>
</div>